<div class="container">
    <div class = "row">
    <div class="input-group col-md-8">
        <input class="form-control" type="text" placeholder="Search" aria-label="Search">
        <button class="btn btn-primary">Search</button>
    </div>
    </div>
    <br><br>
    <div class="row">
        {{#each data }}
            <div class="card col-lg-4 col-sm-6 col-xs-12" style="width: 18rem;">
                <div class="card-header">
                    <b>{{id}}: {{first_name}} {{last_name}}</b>
                </div>
                <div class="card-body">
                    <p class="card-text">
                        <td>{{phone}}</td><br>
                        <td>{{email}}</td><br>
                        <td>{{city}}, {{state}} {{zipcode}}</td><br>
                        <td>{{age}}</td><br>
                        <td>{{desired_pace}}</td><br>
                        {{#if admin}}
                            <td>Admin</td>
                        {{/if}}
                    </p>
                    <a href="../user/profile/{{id}}" class="btn btn-primary">View Profile</a>
                </div>
            </div>
        {{/each}}
    </div>
</div>